{
  "name": "draw-html-to-canvas",
  "version": "0.0.1",
  "description": "根据html+css规范绘制 网页到canvas",
  "main": "dist/index.cjs.js",
  "browser": "dist/index.umd.js",
  "types": "types",
  "files": [
    "readme.md",
    "dist/**/*",
    "example/**/*",
    "types/**/*"
  ],
  "publishConfig": {},
  "scripts": {
    "dev": "rollup -c rollup.config.js --watch",
    "build": "rm -rf dist && rollup -c rollup.config.js && npm run build:d.ts",
    "build:d.ts": "rm -rf types && tsc -t es5 --declaration --outDir ./types ./src/index.ts && rm -rf ./types/*.js"
  },
  "keywords": [
    "canvas",
    "html",
    "html-to-canvas",
    "html2canvas",
    "dhtml2canvas",
    "poster",
    "海报"
  ],
  "author": "nap.liu",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "rollup": "^2.71.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
